    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "<rootDir>/src/config/jest/setup.ts"
    ],
    "moduleNameMapper": {
